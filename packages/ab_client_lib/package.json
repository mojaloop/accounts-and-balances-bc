{
    "name": "@mojaloop/accounts-and-balances-bc-client-lib",
    "version": "0.6.0-alpha.12",
    "description": "Accounts and Balances BC - Client Library",
    "repository": {
        "type": "git",
        "url": "https://github.com/mojaloop/accounts-and-balances-bc/tree/main/packages/ab-client-lib"
    },
    "bugs": {
        "url": "https://github.com/mojaloop/accounts-and-balances-bc/issues"
    },
    "license": "Apache-2.0",
    "contributors": [
        "Pedro Sousa Barreto <pedrob@crosslaketech.com>"
    ],
    "private": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "npm run clean:npm; npm run clean:dist",
        "clean:dist": "rm -rf dist",
        "clean:npm": "rm -rf node_modules",
        "clean:types": "rm -fr src/types/proto-gen",
        "dep:check": "ncu -e 2",
        "dep:update": "ncu -u",
        "lint": "eslint ./src",
        "lint:fix": "eslint ./src --fix",
        "build": "tsc",
        "test:unit": "echo \"for now, this lib doesn't have unit tests\""
    },
    "engines": {
        "node": ">=20.10.0"
    },
    "devDependencies": {
        "tslib": "~2.6.2"
    },
    "dependencies": {
        "@mojaloop/security-bc-client-lib": "~0.5.12",
        "@mojaloop/security-bc-public-types-lib": "~0.5.6",
        "@mojaloop/accounts-and-balances-bc-public-types-lib": "~0.6.0-alpha.2",
        "@mojaloop/logging-bc-public-types-lib": "~0.5.4",
        "@mojaloop/platform-configuration-bc-public-types-lib": "~0.5.2",
        "@mojaloop/platform-shared-lib-observability-types-lib": "~0.5.5",
        "@grpc/grpc-js": "~1.10.6",
        "@grpc/proto-loader": "~0.7.12",
        "tigerbeetle-node": "~0.15.3"
