{
  "name": "@mojaloop/accounts-and-balances-bc-builtin-ledger-grpc-client-lib",
  "version": "0.0.2",
  "description": "Accounts and Balances BC - gRPC Client Library",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mojaloop/accounts-and-balances-bc/tree/main/packages/builtin-ledger-grpc-client-lib"
  },
  "bugs": {
    "url": "https://github.com/mojaloop/accounts-and-balances-bc/issues"
  },
  "contributors": [
    "Gon√ßalo Garcia <goncalogarcia99@gmail.com>",
    "Pedro Sousa Barreto <pedrob@crosslaketech.com>"
  ],
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/"
  ],
  "private": true,
  "scripts": {
    "clean:dist": "rm -fr dist",
    "clean:types": "rm -fr ./src/types/",
    "dep:check": "ncu -e 2",
    "dep:upgrade": "ncu -u",
    "audit:check": "npm audit",
    "audit:fix": "npm audit fix",
    "lint": "eslint src",
    "gen-types": "sh ./gen-types.sh",
    "prebuild": "npm run gen-types",
    "build": "tsc",
    "postbuild": "cp ./src/builtin_ledger.proto ./dist/",
    "test:unit": "echo \"for now, builtin-ledger-grpc-client-lib doesn't have unit tests\"",
    "test:unit-single": "jest --coverage false -t"
  },
  "devDependencies": {
    "@grpc/proto-loader": "~0.7.4",
    "@mojaloop/accounts-and-balances-bc-public-types-lib": "~0.0.4"
  },
  "dependencies": {
    "@grpc/grpc-js": "~1.8.0",
    "@mojaloop/logging-bc-public-types-lib": "~0.1.14"
  }
}
