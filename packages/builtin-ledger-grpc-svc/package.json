{
    "name": "@mojaloop/accounts-and-balances-bc-builtin-ledger-grpc-svc",
    "version": "0.2.1",
    "description": "Accounts and Balances BC - Builtin Ledger gRPC Service",
    "repository": {
        "type": "git",
        "url": "https://github.com/mojaloop/accounts-and-balances-bc/tree/main/packages/builtin-ledger-grpc-svc"
    },
    "bugs": {
        "url": "https://github.com/mojaloop/accounts-and-balances-bc/issues"
    },
    "license": "Apache-2.0",
    "contributors": [
        "Gon√ßalo Garcia <goncalogarcia99@gmail.com>",
        "Pedro Sousa Barreto <pedrob@crosslaketech.com>"
    ],
    "private": true,
    "mojaloop": {
        "publish_to_dockerhub": true
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "clean": "npm run clean:npm; npm run clean:dist",
        "clean:dist": "rm -rf dist",
        "clean:npm": "rm -rf node_modules",
        "dep:check": "ncu -e 2",
        "dep:update": "ncu -u",
        "lint": "eslint ./src",
        "lint:fix": "eslint ./src --fix",
        "build": "tsc",
        "postbuild": "cp src/domain/currencies.json dist/domain",
        "test:unit": "jest",
        "start": "npm run service",
        "start:dev": "npm run build && AUDIT_KEY_FILE_PATH=./dist/tmp_key_file npm run start",
        "service": "node dist/application/index.js"
    },
    "devDependencies": {
        "@mojaloop/accounts-and-balances-bc-shared-mocks-lib": "~0.0.1",
        "@types/jest": "^29.5.1",
        "@types/uuid": "^9.0.2",
        "eslint": "^8.41.0",
        "jest": "^29.5.0",
        "nock": "^13.3.1",
        "npm-check-updates": "~16.10.12",
        "ts-jest": "^29.1.0",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "@mojaloop/platform-configuration-bc-client-lib": "~0.3.4",
        "@mojaloop/platform-configuration-bc-public-types-lib": "~0.3.3",
        "@mojaloop/platform-shared-lib-nodejs-kafka-client-lib": "~0.3.65",
        "@mojaloop/security-bc-client-lib": "~0.2.2",
        "@mojaloop/security-bc-public-types-lib": "~0.2.1",
        "@grpc/grpc-js": "~1.8.14",
        "@grpc/proto-loader": "~0.7.7",
        "@mojaloop/accounts-and-balances-bc-builtin-ledger-grpc-client-lib": "*",
        "@mojaloop/accounts-and-balances-bc-public-types-lib": "*",
        "@mojaloop/auditing-bc-client-lib": "~0.0.14",
        "@mojaloop/auditing-bc-public-types-lib": "~0.0.15",
        "@mojaloop/logging-bc-client-lib": "~0.1.24",
        "@mojaloop/logging-bc-public-types-lib": "~0.1.19",
        "@mojaloop/platform-shared-lib-observability-client-lib": "~0.1.8",
        "@mojaloop/platform-shared-lib-observability-types-lib": "~0.1.7",
        "express": "^4.18.2",
        "ioredis": "~5.3.2",
        "mongodb": "~5.5.0",
        "tslib": "~2.5.2",
        "uuid": "~9.0.0"
    }
}
