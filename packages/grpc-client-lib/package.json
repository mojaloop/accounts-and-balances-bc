{
  "name": "@mojaloop/accounts-and-balances-bc-grpc-client-lib",
  "version": "0.0.9",
  "description": "Accounts and Balances BC - gRPC Client Library",
  "repository": {
    "type": "git",
    "url": "https://github.com/mojaloop/accounts-and-balances-bc/tree/main/packages/grpc-client-lib"
  },
  "bugs": {
    "url": "https://github.com/mojaloop/accounts-and-balances-bc/issues"
  },
  "license": "Apache-2.0",
  "contributors": [
    "Gon√ßalo Garcia <goncalogarcia99@gmail.com>",
    "Pedro Sousa Barreto <pedrob@crosslaketech.com>"
  ],
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean:dist": "rm -fr dist",
    "clean:log": "rm -f *.log",
    "clean:types": "rm -fr src/types/proto-gen",
    "dep:check": "ncu -e 2",
    "dep:upgrade": "ncu -u",
    "lint": "eslint src test",
    "gen-types": "sh gen-types.sh",
    "prebuild": "npm run gen-types",
    "build": "tsc",
    "postbuild": "cp src/accounts_and_balances.proto dist",
    "test:unit": "echo \"for now, grpc-client-lib doesn't have unit tests\"",
    "prepublishOnly": "find dist -name \"*.js.map\" | xargs rm -f",
    "pub": "npm run clean:debug && npm version patch && npm publish --access public"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "devDependencies": {},
  "dependencies": {
    "@grpc/grpc-js": "~1.8.4",
    "@grpc/proto-loader": "~0.7.4",
    "@mojaloop/accounts-and-balances-bc-public-types-lib": "~0.0.8",
    "@mojaloop/logging-bc-public-types-lib": "~0.1.14",
    "tslib": "~2.4.1"
  }
}
