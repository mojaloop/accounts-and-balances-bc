{
  "name": "@mojaloop/accounts-and-balances-bc-grpc-svc",
  "version": "0.0.1",
  "description": "Accounts and Balances BC - gRPC Service",
  "license": "Apache-2.0",
  "homepage": "https://github.com/mojaloop/accounts-and-balances-bc/tree/main/packages/grpc-svc",
  "bugs": {
    "url": "https://github.com/mojaloop/accounts-and-balances-bc/issues#readme"
