{
  "name": "@mojaloop/accounts-and-balances-bc-grpc-svc",
  "version": "0.1.8",
  "description": "Accounts and Balances BC - gRPC service",
  "repository": {
    "type": "git",
    "url": "https://github.com/mojaloop/accounts-and-balances-bc/tree/main/packages/accounts-and-balances-grpc-svc"
  },
  "bugs": {
    "url": "https://github.com/mojaloop/accounts-and-balances-bc/issues"
  },
  "license": "Apache-2.0",
  "contributors": [
    "Gon√ßalo Garcia <goncalogarcia99@gmail.com>",
    "Pedro Sousa Barreto <pedrob@crosslaketech.com>"
  ],
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean:npm": "rm -fr node_modules",
    "clean:dist": "rm -fr dist",
    "clean:log": "rm -f *.log",
    "clean:types": "rm -fr src/types",
    "dep:check": "ncu -e 2",
    "dep:update": "ncu -u",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "build": "tsc",
    "postbuild": "cp src/domain/currencies.json dist/domain",
    "test:unit": "jest",
    "start": "npm run service",
    "start:dev": "npm run build && AUDIT_KEY_FILE_PATH=./dist/tmp_key_file npm run start",
    "service": "node dist/application/index.js"
  },
  "devDependencies": {
    "@mojaloop/accounts-and-balances-bc-shared-mocks-lib": "~0.0.1",
    "eslint": "^8.33.0",
    "@types/jest": "^29.4.0",
    "jest": "^29.4.1",
    "nock": "^13.3.0",
    "npm-check-updates": "~16.6.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@grpc/grpc-js": "~1.8.8",
    "@grpc/proto-loader": "~0.7.4",
    "@mojaloop/accounts-and-balances-bc-grpc-client-lib": "*",
    "@mojaloop/accounts-and-balances-bc-public-types-lib": "*",
    "@mojaloop/logging-bc-public-types-lib": "~0.1.14",
    "@mojaloop/logging-bc-client-lib": "~0.1.17",
    "@mojaloop/security-bc-client-lib": "~0.1.16",
    "@mojaloop/security-bc-public-types-lib": "~0.1.16",
    "mongodb": "~5.0.1",
    "tslib": "~2.5.0"
  }
}
