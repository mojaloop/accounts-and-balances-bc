  "scripts": {
    "clean:dist": "rm -fr dist",
    "clean:npm": "rm -fr node_modules",
    "clean:package-lock": "rm -f package-lock.json",
    "clean:coverage": "rm -fr coverage",
    "clean:all": "npm run clean:dist && npm run clean:npm && npm run clean:package-lock && npm run clean:coverage",
    "dep:check": "ncu -e 2",
    "dep:upgrade": "ncu -u",
    "audit:check": "npm audit",
    "audit:fix": "npm audit fix",
    "lint": "eslint src",
    "build": "tsc",
    "test:unit": "jest",
    "test:unit-single": "jest --coverage false -t",
    "publish": "npm version patch && npm publish --tag latest --access public"
  },
  "devDependencies": {
    "nock": "^13.2.9"
  },
  "dependencies": {
    "@mojaloop/logging-bc-public-types-lib": "^0.1.10",
