{
    "name": "@mojaloop/accounts-and-balances-bc-public-types-lib",
    "version": "0.6.0-alpha.2",
    "description": "Accounts and Balances BC - Public Types Library",
    "repository": {
        "type": "git",
        "url": "https://github.com/mojaloop/accounts-and-balances-bc/tree/main/packages/public-types-lib"
    },
    "bugs": {
        "url": "https://github.com/mojaloop/accounts-and-balances-bc/issues"
    },
    "license": "Apache-2.0",
    "contributors": [
        "Gon√ßalo Garcia <goncalogarcia99@gmail.com>",
        "Pedro Sousa Barreto <pedrob@crosslaketech.com>"
    ],
    "private": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "npm run clean:npm; npm run clean:dist",
        "clean:dist": "rm -rf dist",
        "clean:npm": "rm -rf node_modules",
        "dep:check": "ncu -e 2",
        "dep:update": "ncu -u",
        "lint": "eslint ./src",
        "lint:fix": "eslint ./src --fix",
        "build": "tsc",
        "test:unit": "echo \"public-types-lib doesn't have unit tests\"",
        "gen-types": "sh gen-types.sh",
        "prebuild": "npm run gen-types",
        "postbuild": "mkdir -p dist/proto; cp ./proto_files/*.proto dist/proto/"
    },
    "engines": {
        "node": ">=20.10.0"
    },
    "devDependencies": {
        "tslib": "~2.6.2",
        "@grpc/grpc-js": "~1.10.6",
        "@grpc/proto-loader": "~0.7.12"
    }
}
